/ret -= 0.02;//*= 0.95; //or try: ret -= 0.004;\n}","comp_hlsl":"shader_body\n{\n    float2 uv_echo = (uv - 0.5)*0.993*float2(-1,1) + 0.5;\n    ret = lerp( tex2D(sampler_main, uv).xyz, \n                tex2D(sampler_main, uv_echo).xyz, \n                0.50 \n              ); //video echo\n    ret *= 1.00 + (treb_att + (mid_att * .5)); //gamma\n    ret = sqrt(ret); //brighten\n    ret *= ret + (treb_att * .05); //darken\n//ret *= ret + (treb_att * .5);\n \n//ret = (sin(time)) + sin(time) -ret;\n}"}