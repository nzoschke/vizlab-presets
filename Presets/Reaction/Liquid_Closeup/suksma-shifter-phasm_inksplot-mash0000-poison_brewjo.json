n  vec4 tmpvar_1;\n  tmpvar_1.w = 1.0;\n  tmpvar_1.xyz = (texture (sampler_main, uv).xyz - 0.004);\n  ret = tmpvar_1.xyz;\n }","comp":"","warp_hlsl":"shader_body\n{\n    // sample previous frame\n    ret = tex2D( sampler_main, uv ).xyz;\n\n    // darken over time\n    ret -= 0.004;\n}","comp_hlsl":""}