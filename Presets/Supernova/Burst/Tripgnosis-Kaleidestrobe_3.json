id_att)*.333333;\n\nra = 10;\ntreb_avg = tic*(treb_avg*(1/tic - ra) + ra*treb);\nmid_avg = tic*(mid_avg*(1/tic - ra) + ra*mid);\nbass_avg = tic*(bass_avg*(1/tic - ra) + ra*bass);\nvav = tic*(vav*(1/tic - ra) + ra*(bass+treb+mid)*.33333);\n\ntt = tt + tic*treb;\nmt = mt + tic*mid;\nbt = bt + tic*bass;\nvt = vt + tic*vav;\n\nsp = abs(vav - slide)*.1;\nslide = if(above(slide,vav),slide-tic*sp,slide+tic*sp) + (1-toc)*vav*.2;\ntoc = 1;\n\n// for waves\nq6 = bt;\nq7 = mt;\nq8 = tt;\nq3 = slide;\n\nq2 = vt*.5; // speed of opacity cycle\n\nvrt = vrt + tic*min(1,max(0.1,2-vav));\n\nq4 = vrt*6; // = timecycle of hue\n\nzoom = .9 + pow(vav,4)*.22;\n\nmonitor = vav;","pixel_eqs_eel":"","warp":" shader_body { \n  vec3 ret_1;\n  vec4 tmpvar_2;\n  tmpvar_2 = texture (sampler_fc_main, uv);\n  ret_1 = tmpvar_2.xyz;\n  if ((((tmpvar_2.x + tmpvar_2.y) + tmpvar_2.z) < ((\n    ((mid + bass) + treb)\n   / 3.0) * 0.3))) {\n    ret_1 = (tmpvar_2.xyz + vec3(0.6, 0.6, 0.6));\n  } else {\n    ret_1 = ((ret_1 - 0.005) * 0.99);\n  };\n  vec4 tmpvar_3;\n  tmpvar_3.w = 1.0;\n  tmpvar_3.xyz = ret_1;\n  ret = tmpvar_3.xyz;\n }","comp":" shader_body { \n  float tmpvar_1;\n  tmpvar_1 = abs(((\n    fract((((ang / 6.28) * 7.0) + (time * 0.05)))\n   * 2.0) - 1.0));\n  vec2 tmpvar_2;\n  tmpvar_2.x = cos(tmpvar_1);\n  tmpvar_2.y = sin(tmpvar_1);\n  vec4 tmpvar_3;\n  tmpvar_3.w = 1.0;\n  tmpvar_3.xyz = (-0.3 + (1.7 * texture (sampler_main, (0.5 + \n    ((pow (rad, 0.5) * 0.5) * tmpvar_2)\n  )).xyz));\n  ret = tmpvar_3.xyz;\n }","warp_hlsl":"shader_body\n{\n    // sample previous frame\n    ret = tex2D( sampler_fc_main, uv ).xyz;\n    //ret = lerp(GetBlur1(uv)*1,ret,ret);\n    // darken (decay) over time\n    if (ret.x+ret.y+ret.z < (0+((mid+bass+treb)/3)*.3)){\n       ret.x = ret.x+.6;\n       ret.y = ret.y+.6;\n       ret.z = ret.z+.6;\n       }else{\n       ret = (ret - .005)*0.99; //or try: ret -= 0.004;\n    }\n}","comp_hlsl":"shader_body\n{\n  //rad = frac(rad*2);\n  rad = pow(rad,0.5)*1;\n\n\n  float ang2 = frac(ang/6.28*7 + time*0.05);\n  ang2 = abs(ang2*2-1);\n  float2 uv2 = 0.5 + rad*0.5*float2(cos(ang2),sin(ang2));    \n\n  uv = uv2;\n  //uv = lerp(uv, uv2, 0.5 + 0.5*cos(ang + time*3));\n\n\n  ret = tex2D(sampler_main, uv).xyz;\n\n  //ret = lum(ret);\n  //ret = pow(ret,float3(0.7,1.3,1.6));\n\n  ret = -0.3 + 1.7*ret;\n}\n\n"}